{
  "private": true,
  "name": "nyx-launcher",
  "description": "Nyx MuOnline Launcher",
  "version": "0.0.1",
  "author": "Dea7h",
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "build": "nextron build",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "electron-serve": "^0.4.1",
    "electron-store": "^5.1.1",
    "portscanner": "^2.2.0",
    "rss-parser": "^3.8.0",
    "styled-components": "^5.1.0",
    "winreg": "^1.2.4"
  },
  "devDependencies": {
    "@types/node": "^13.9.5",
    "@types/portscanner": "^2.1.0",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.1.0",
    "@types/winreg": "^1.2.30",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "next": "^9.3.2",
    "nextron": "^5.14.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.8.3"
  },
  "build": {
    "appId": "com.nyx.launcher",
    "productName": "Nyx MuOnline Launcher",
    "copyright": "Copyright Â© 2020 Dea7h",
    "win": {
      "target": "portable",
      "icon": "resources/icon.ico"
